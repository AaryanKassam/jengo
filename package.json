{
  "name": "jengo",
  "version": "1.0.0",
  "description": "A C2C volunteer marketplace - MERN Stack",
  "private": true,
  "scripts": {
    "dev": "PATH=\"/usr/local/bin:/opt/homebrew/bin:$PATH\" concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "PATH=\"/usr/local/bin:/opt/homebrew/bin:$PATH\" cd server && npm run dev",
    "dev:client": "PATH=\"/usr/local/bin:/opt/homebrew/bin:$PATH\" cd client && npm run dev",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "cd server && npm start",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "mern",
    "volunteer",
    "marketplace"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.3",
    "mongoos": "^0.0.1-security",
    "mongoose": "^9.1.4"
  }
}
